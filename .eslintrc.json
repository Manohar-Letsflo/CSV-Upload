{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "globals": {
    "logger": "readonly",
    "ENVIRONMENT": "readonly",
    "importVariables": "readonly"
  },
  "extends": "eslint:recommended",
  "parserOptions": { "ecmaVersion": 13 },
  "rules": {
    // "no-unused-vars": "off",
    "no-async-promise-executor": "off",
    "no-prototype-builtins": "off",
    "no-case-declarations": "off"
  }
}
